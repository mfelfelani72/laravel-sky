{
  "name": "mfelfelani72/laravel-sky",
  "description": "My Laravel starter template",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0",
    "laravel/tinker": "^2.10"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.24",
    "laravel/sail": "^1.41",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.6",
    "phpunit/phpunit": "^11.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/app/",
      "Database\\Factories\\": "src/database/factories/",
      "Database\\Seeders\\": "src/database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "src/tests/"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php scripts/setup.php"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
